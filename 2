#include <stdio.h>

int main() {
    int num1, num2;

    printf("Digite o valor do primeiro número: ");
    scanf("%d", &num1);

    printf("Digite o valor do segundo número: ");
    scanf("%d", &num2);
    
    if (&num1 > &num2) {
        printf("O endereço do primeiro número é maior: %p\n", (void *)&num1);
    } else if (&num2 > &num1) {
        printf("O endereço do segundo número é maior: %p\n", (void *)&num2);
    } else {
        printf("Os endereços dos números são iguais.\n");
    }

    return 0;
}
