#include <stdio.h>

void encontrarMaiorMenor(int *a, int *b) {
    if (*a < *b) {
        int temp = *a;
        *a = *b;
        *b = temp;
    }
}

int main() {
    int valor1, valor2;

    printf("Digite o primeiro valor: ");
    scanf("%d", &valor1);

    printf("Digite o segundo valor: ");
    scanf("%d", &valor2);

    encontrarMaiorMenor(&valor1, &valor2);

    printf("Maior valor: %d\nMenor valor: %d\n", valor1, valor2);

    return 0;
}

